<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:jdbc="http://www.springframework.org/schema/jdbc"
	xmlns:jee="http://www.springframework.org/schema/jee"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/jdbc http://www.springframework.org/schema/jdbc/spring-jdbc-3.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd
		http://www.springframework.org/schema/jee http://www.springframework.org/schema/jee/spring-jee-3.0.xsd">
	
	<bean id="activityService"
        class="com.tvelykyy.afeeder.service.ActivityServiceImpl">
    </bean>
    <bean id="activityDAO"
        class="com.tvelykyy.afeeder.dao.ActivityDAOImpl">
    </bean>
    
    <bean id="userService"
        class="com.tvelykyy.afeeder.service.UserServiceImpl">
    </bean>
    <bean id="userDAO"
        class="com.tvelykyy.afeeder.dao.UserDAOImpl">
    </bean>
    
    <bean id="groupService"
        class="com.tvelykyy.afeeder.service.GroupServiceImpl">
    </bean>
    <bean id="groupDAO"
        class="com.tvelykyy.afeeder.dao.GroupDAOImpl">
    </bean>
    
    <import resource="file:**/WEB-INF/spring/data.xml" />

</beans>
